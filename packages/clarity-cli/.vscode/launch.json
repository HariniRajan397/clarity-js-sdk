    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Setup from_source",
        "program": "${workspaceFolder}/bin/run",
        "args": [
          "setup",
          "--overwrite",
          "--from_source"
        ],
        "console": "integratedTerminal",
        "preLaunchTask": "tsc-build-lib"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Setup from_dist",
        "program": "${workspaceFolder}/bin/run",
        "args": [
          "setup",
          "--overwrite"
        ],
        "console": "integratedTerminal",
        "preLaunchTask": "tsc-build-lib"
      }
    ]
}
